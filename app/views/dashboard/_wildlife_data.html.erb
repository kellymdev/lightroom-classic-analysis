<h2>Wildlife Data</h2>
<dl>
  <dt>Cameras:</dt>
  <dd>
    <ul>
      <% @wildlife_data[:cameras].each do |camera| %>
        <li><%= camera %></li>
      <% end %>
    </ul>
  </dd>

  <dt>Lenses:</dt>
  <dd>
    <ul>
      <% @wildlife_data[:lenses].each do |lens| %>
        <li><%= lens %></li>
      <% end %>
    </ul>
  </dd>

  <dt>Camera Lens Combinations:</dt>
  <dd>
    <ul>
      <% @wildlife_data[:camera_lens_combinations].each do |combination| %>
        <li><%= combination %></li>
      <% end %>
    </ul>
  </dd>

  <dt>Focal Lengths:</dt>
  <dd>
    <ul>
      <% @wildlife_data[:focal_lengths].each do |focal_length| %>
        <li><%= focal_length %>mm</li>
      <% end %>
    </ul>
  </dd>

  <dt>Isos:</dt>
  <dd>
    <ul>
      <% @wildlife_data[:isos].each do |iso| %>
        <li><%= iso %></li>
      <% end %>
    </ul>
  </dd>

  <dt>Shutter Speeds:</dt>
  <dd>
    <ul>
      <% @wildlife_data[:shutter_speeds].each do |shutter_speed| %>
        <li>1/<%= shutter_speed %></li>
      <% end %>
    </ul>
  </dd>

  <dt>Month:</dt>
  <dd><%= @wildlife_data[:month] %></dd>

  <dt>Year:</dt>
  <dd><%= @wildlife_data[:year] %></dd>
</dl>